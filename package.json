{
  "name": "poll-datasets",
  "version": "1.0.0",
  "description": "AWS Lambda function that polls various external sources for dataset updates.",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "clean": "rm -rf ./build ./dist",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint ./src",
    "test": "jest",
    "dist": "npm run clean && npm run build && zip-build ./build/webpack ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/cinira-llc/poll-datasets.git"
  },
  "author": "Matt Jensen <mjensen@cinira.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/cinira-llc/poll-datasets/issues"
  },
  "homepage": "https://github.com/cinira-llc/poll-datasets#readme",
  "dependencies": {
    "lodash": "^4"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8",
    "@types/jest": "^29",
    "@types/lodash": "^4",
    "@typescript-eslint/eslint-plugin": "^6",
    "@typescript-eslint/parser": "^6",
    "eslint": "^8",
    "eslint-config-prettier": "^9",
    "jest": "^29",
    "jest-junit": "^16",
    "prettier": "^3",
    "semantic-release": "^22",
    "ts-loader": "^9",
    "ts-jest": "^29",
    "typescript": "^5",
    "webpack": "^5",
    "webpack-cli": "^5",
    "zip-build": "^1"
  },
  "peerDependencies": {
    "aws-sdk": "^2"
  },
  "release": {
    "plugins": [
      "@semantic-release/github",
      "@semantic-release/npm"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "reporters": [
      [
        "jest-junit",
        {
          "outputDirectory": "./build/report",
          "outputName": "test-results.xml"
        }
      ]
    ]
  }
}
